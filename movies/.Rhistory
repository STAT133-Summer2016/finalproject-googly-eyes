TossNCoins = function(n){
heads = sample(c(1,0), size = x, replace = T, prob = c(0.5, 0.5))
sum(heads)/n
}
tosses = rerun(1000, map(1:100, TossNCoins))
DrawGraph = function(n){
proportions = map(1:1000, function(i){tosses[[i]][n]}) %>% unlist()
ggplot() + geom_histogram(aes(x=proportions, y = ..density..), bins = 30)
}
map(1:100, DrawGraph)
